<div id="blog">
<% @categories.images.each do |image| %>
<div id="category-image-overview">
	
<div id="image-meta">
<span class="image-title"><%= image.title %></span><span class="image-date"><%= image.created_at.strftime("%d %b %Y") %></span>

</div>

<%= link_to image_tag(image.photo.url(:category)), :controller => 'images', :action => 'show', :id => image %>


</div>
<% end %>



</div>

